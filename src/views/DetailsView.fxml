<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="350.0" prefWidth="550.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.DetailsViewController">
   <children>
      <Label alignment="CENTER" layoutX="100.0" layoutY="18.0" prefHeight="36.0" prefWidth="350.0" text="Member Details" AnchorPane.topAnchor="18.0">
         <font>
            <Font name="Times New Roman" size="25.0" />
         </font>
      </Label>
      <TextArea fx:id="textArea" editable="false" layoutX="14.0" layoutY="56.0" prefHeight="286.0" prefWidth="344.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0">
         <font>
            <Font name="Times New Roman" size="14.0" />
         </font>
      </TextArea>
      <ComboBox fx:id="comboBox" editable="true" layoutX="307.0" layoutY="119.0" prefHeight="25.0" prefWidth="183.0" AnchorPane.rightAnchor="10.0" />
      <Label layoutX="307.0" layoutY="83.0" prefHeight="36.0" prefWidth="183.0" text="Select a Family Member : " AnchorPane.rightAnchor="10.0">
         <font>
            <Font name="Times New Roman" size="15.0" />
         </font>
      </Label>
      <Button fx:id="displayButton" layoutX="323.0" layoutY="199.0" mnemonicParsing="false" onAction="#onDisplayButton" prefHeight="36.0" prefWidth="153.0" text="Display" AnchorPane.rightAnchor="25.0">
         <font>
            <Font name="Times New Roman" size="16.0" />
         </font>
      </Button>
      <Button fx:id="cancelButton" layoutX="322.0" layoutY="260.0" mnemonicParsing="false" onAction="#onCancelButton" prefHeight="36.0" prefWidth="153.0" text="Cancel" AnchorPane.rightAnchor="25.0">
         <font>
            <Font name="Times New Roman" size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
